{
  "name": "reduxstagram",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.1",
    "react-scripts": "1.0.14",
    "stylus": "^0.54.5"
  },
  "scripts": {
    "start": "run-p dev styles:watch",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "prebuild": "npm run styles",
    "eject": "react-scripts eject",
    "styles": "stylus -I ./node_modules ./src/styles -o ./src/styles/",
    "styles:watch": "stylus -I ./node_modules -w ./src/styles -o ./src/styles/"
  }
}
